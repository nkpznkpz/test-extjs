<%= javascript_include_tag ex_path+'ux/GroupSummary' %>
<%= stylesheet_link_tag ex_path+'ux/css/GroupSummary' %>
<%= javascript_include_tag 'application' %>
<script type="text/javascript">
  Ext.BLANK_IMAGE_URL = 'javascripts/extjs/resources/images/default/s.gif';
  //rails security key
  Ext.Ajax.extraParams = {authenticity_token: '<%= form_authenticity_token %>'};
</script>